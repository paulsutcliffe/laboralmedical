				  <% end %>
				</li>

                   	 	<li class="fotos">
				  <%= link_to_unless_current("",  { :controller => "albums", :action => "adminlist"}) do %>
					<span>Fotos</span>
				  <% end %>
				</li>

                		<li class="staff">
				  <%= link_to_unless_current("",  { :controller => "members", :action => "adminlist"}) do %>
                                    <span>Staff</span>
                                  <% end %>
				</li>

                    		<li class="estrategicas">
				  <%= link_to_unless_current("",  { :controller => "alliances", :action => "adminlist"}) do %>
                                    <span>Alianzas Estrat√©gicas</span>
                                  <% end %>
				</li>
                	</ul>
    			</nav>
            </section>
            
           
        </div>
    </header>










	<div id="main" role="main" class="not-front staff">
	    	<section id="preface-bottom" >
	        	<div class="container_16" id="admin">
  <%= yield %>
				</div>
			</section>
  	</div>


  
   
  </div> <!--! end of #container -->












  <!-- JavaScript at the bottom for fast page loading -->

  <%= javascript_include_tag :defaults, :cache => true %>
